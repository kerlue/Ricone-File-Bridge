<?xml version="1.0" encoding="UTF-8"?><!--/** * @author      Schillaci "Dwayne" McInnis <dmcinnis@lhric.org> * @version     1.0 * @since       Jul 7, 2016 * Filename		config.xml */--><configSettings>    <!--out requirements-->    <output_schema>csv</output_schema>    <!--Output File Format (CSV, XML or JSON)-->    <output_destination>        <!--output destination-->        <output_export>local</output_export>        <!--Enter output destination (local or SFTP)-->        <output_path></output_path>        <!--Enter output destination path (ex: C:\Users\Desktop , sftp.pifdata.net )-->        <output_folder_title>SCHOOL</output_folder_title>        <!--Enter output folder title (ex: Districts )-->        <sftp_port>22</sftp_port>        <!--If exporting files to SFTP enter port number-->        <sftp_username>tester</sftp_username>        <!--If exporting files to SFTP enter username-->        <sftp_password>password</sftp_password>        <!--If exporting files to SFTP enter password-->        <zip_enabled>false</zip_enabled>        <log_enabled>true</log_enabled>    </output_destination>    <!--Input requirements-->    <authentication_settings>        <auth_url>test</auth_url>        <client_id> test</client_id>        <provider_id>test </provider_id>        <navigation_page_size> test </navigation_page_size>        <client_secret>test </client_secret>    </authentication_settings>    <!--Required data to search for. The tag represents text file name. -->    <!-- column_name represents the name for each column. -->    <required_data>        <CODE_DISTRICT column_name = "district_code,district_name,address_1,address_2,city,state,zip">	       xLeas refId, xLeas leaName,xLeas line1,xLeas line2,xLeas city,xLeas stateProvince,xLeas postalCode       </CODE_DISTRICT>        <SCHOOL column_name = "school_code,school_name,district_code,address_1,address_2,city,state,zip">           xSchool refId,xSchool leaName,xLea refId,xSchool line1,xSchool line2,xSchool city,xSchool stateProvince,xSchool postalCode       </SCHOOL>        <PIF_SCTION column_name = "native_section_code,school_code,date_start,date_end,school_year,section_name,section_number">          xRoster refId, xSchool refId,null,null,xRoster schoolYear,xRoster subject,xRoster schoolSectionId         </PIF_SCTION>        <STAFF column_name = "staff_code,last_name,first_name,middle_name,email,title,staff_number,username,password">           xStaff refId,xStaff familyName,xStaff givenName,xStaff middleName,xStaff email,xStaff prefix,null,null       </STAFF>        <STUDENT column_name = "student_code,last_name,first_name,middle_name,email,student_number,username,password">           xStudent refId,xStudent familyName,xStudent givenName,xStudent middleName,xStudent email,xStudent refId,null,null           </STUDENT>        <PIF_SECTION_STAFF column_name = "section_teacher_code,staff_code,native_section_code,date_start,date_end,school_year,teacher_of_record">          xRoster courseRefId,  xStaff-primaryStaff refId, xRoster refId,null,null, xRoster schoolYear, xStaff-primaryStaff teacherOfRecord        </PIF_SECTION_STAFF>        <PIF_SECTION_STUDENT column_name = "section_student_code,student_code,native_section_code,date_start,date_end,school_year">         null, xStudent refId, xRoster refId, null, null, xRoster schoolYear        </PIF_SECTION_STUDENT>        <ASSIGNMENT column_name = "native_assignment_code,staff_code,school_year,institution_code,date_start,date_end,position_code ">         null, xStaff refId, xRoster schoolYear,xSchool refId, null,null, null       </ASSIGNMENT>    </required_data></configSettings>