<?xml version="1.0" encoding="UTF-8"?><!--/** * @author      Schillaci "Dwayne" McInnis <dmcinnis@lhric.org> * @version     1.0 * @since       Jul 7, 2016 * Filename		config.xml */--><configSettings>    <!--out requirements-->    <output_schema>csv</output_schema>        <!--Output File Format (CSV, XML or JSON)-->    <output_destination>        <!--output destination-->        <output_export>local</output_export>        <!--Enter output destination (local or SFTP)-->        <output_path>/Users/training/Desktop</output_path>        <!--Enter output destination path (ex: C:\Users\Desktop , sftp.pifdata.net )-->        <output_folder_title>SCHOOL DATA</output_folder_title>        <!--Enter output folder title (ex: Districts )-->        <sftp_port>22</sftp_port>        <!--If exporting files to SFTP enter port number-->        <sftp_username>tester</sftp_username>        <!--If exporting files to SFTP enter username-->        <sftp_password>password</sftp_password>        <!--If exporting files to SFTP enter password-->        <zip_enabled>false</zip_enabled>        <log_enabled>true</log_enabled>    </output_destination>        <!--Input requirements-->        <authentication_settings>        <auth_url>https://auth.ricone.org/login</auth_url>        <client_id>RICOneFileBridge</client_id>        <provider_id>test </provider_id>        <navigation_page_size> test </navigation_page_size>        <client_secret>redacted</client_secret>    </authentication_settings>        <!--Required data to search for. The tag represents text file name. -->    <!-- column_name represents the name for each column. -->         <filter><filter_by>Lea</filter_by>      <filter_refid>15077B52-7D2A-4855-B41F-37FBA242522E</filter_refid>      <filter_grades></filter_grades></filter>             <required_data>            <CODE_DISTRICT column_name = "district_code,district_name,address_1,address_2,city,state,zip">xLea RefId, LeaName, Address Line1, Address Line2, Address City, Address StateProvince, Address PostalCode</CODE_DISTRICT>        <SCHOOL column_name = "school_code,school_name,district_code,address_1,address_2,city,state,zip,REMOVETHIS-grades">xSchool RefId, SchoolName, LeaRefId, Address Line1, Address Line2, Address City, Address StateProvince, Address PostalCode,GradeLevels GradeLevel</SCHOOL>        <PIF_SCTION column_name = "native_section_code,school_code,date_start,date_end,school_year,section_name,section_number">xRoster RefId,  SchoolRefId, SchoolYear, SchoolYear, SchoolYear, CourseTitle, SchoolSectionId</PIF_SCTION>        <STAFF column_name = "staff_code,last_name,first_name,middle_name,email,title,staff_number,username,password">xStaff RefId, Name FamilyName, Name GivenName, Name MiddleName, Email EmailAddress, Name Prefix,LocalId, Email EmailAddress, RefId</STAFF>        <STUDENT column_name = "student_code,last_name,first_name,middle_name,email,student_number,username,password,REMOVETHIS-Grade">xStudent RefId, Name FamilyName, Name GivenName, Name MiddleName, Email EmailAddress, LocalId,Email EmailAddress,RefId,Enrollment GradeLevel</STUDENT>        <PIF_SECTION_STAFF column_name = "section_teacher_code,staff_code,native_section_code,date_start,date_end,school_year,teacher_of_record">xRoster -andSTAFF SectionRefId StaffPersonReference RefId, StaffPersonReference RefId, SectionRefId,SchoolYear,SchoolYear,SchoolYear, -g -staff -g isTeacherOfRecord</PIF_SECTION_STAFF>                <PIF_SECTION_STUDENT column_name = "section_student_code,student_code,native_section_code,date_start,date_end,school_year">xRoster -andSTUDENT SectionRefId -g -student RefId, -g -student RefId,RefId, SchoolYear, SchoolYear,SchoolYear</PIF_SECTION_STUDENT>         </required_data>    </configSettings>